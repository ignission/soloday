{
	"name": "miipa",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "npx wrangler dev",
		"build": "npx @pandacss/dev codegen && next build",
		"build:worker": "npx @pandacss/dev codegen && rm -rf node_modules/next/dist/compiled/@vercel/og && npx @opennextjs/cloudflare build",
		"start": "next start",
		"deploy": "npm run build:worker && npx wrangler deploy",
		"lint": "npx @biomejs/biome check .",
		"lint:fix": "npx @biomejs/biome check --write .",
		"prepare": "npx @pandacss/dev codegen"
	},
	"dependencies": {
		"@ark-ui/react": "^5.30.0",
		"@auth/d1-adapter": "^1.11.1",
		"@opennextjs/cloudflare": "^1.16.3",
		"@park-ui/panda-preset": "^0.43.1",
		"ical.js": "^2.2.1",
		"next": "16.1.6",
		"next-auth": "5.0.0-beta.30",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"zod": "^4.3.6"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.12",
		"@cloudflare/workers-types": "^4.20260206.0",
		"@pandacss/dev": "file:pandacss-dev-1.8.1.tgz",
		"@types/node": "^20",
		"@types/react": "19.2.13",
		"@types/react-dom": "^19",
		"tsx": "^4.21.0",
		"typescript": "^5",
		"wrangler": "^4.63.0"
	}
}
